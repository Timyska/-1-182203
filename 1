print('введите четырёхзначное число')
h = int(ввод())
print('введите трёхзначное слово')
s = str(ввод())
ключ = str(h) + s
если "usr" в ключе:
 print('введите свой ID номер')
 p = int(ввод())
 если p == h:
 print('Пользователь подтверждён')
 else: print('Неверный ID номер')
элиф "clc" в ключе:
 print("введите первое число")
 per = float(ввод())
 print("введите второе число")
 vtor = float(ввод())
 print("введите третье число")
 tret = float(ввод())
 a = abs (за) * abs (второй) * abs (третий)
 b = за + второй + третий
 c = max(за, второй, третий)
 d = min(за, второй, третий)
 e = c / d
 f = c - d - (за + второй + третий -c - d)
 print('Произведение модулей =', a)
 print('Сумма чисел =', b)
 print('Наибольшее =', c)
 print('Наименьшее =', d)
 print('Деление =', e)
 print('Вычитание =', f)
элиф адм в ключе:
 print('Введите строку')
 strk = str(ввод())
 если len(strk)> 5 и (".exe" в strk или ".cmd" в strk):
 print('Исполняемое действие')
 else: print('Неисполняемое действие')
